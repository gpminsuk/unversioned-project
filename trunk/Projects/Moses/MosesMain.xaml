<Window x:Class="Moses.MosesMain"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:uc="clr-namespace:Moses"
    Title="MosesMain" Height="600" Width="800">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top">
            <Menu>                
                <MenuItem Header="파일"></MenuItem>
            </Menu>
            <ToolBarTray>
                <ToolBar>
                    <TextBlock>A1</TextBlock>
                    <TextBlock>A2</TextBlock>
                    <TextBlock>A3</TextBlock>
                </ToolBar>
                <ToolBar>
                    <TextBlock>B1</TextBlock>
                    <TextBlock>B2</TextBlock>
                    <TextBlock>B3</TextBlock>
                </ToolBar>
            </ToolBarTray>
        </StackPanel>
        <StackPanel DockPanel.Dock="Left">
            <uc:ToolBox>
            </uc:ToolBox>
        </StackPanel>
        <StackPanel DockPanel.Dock="Right">
            <uc:ResourceBrowser x:Name="ResourceBrowser"/>
        </StackPanel>
        <TabControl Name="TabControl">
        </TabControl>
    </DockPanel>
</Window>
